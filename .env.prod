APP_NAME=FixMyMind
APP_ENV=production
APP_KEY=base64:PRODUCTION_KEY_SHOULD_BE_GENERATED_HERE
APP_DEBUG=false
APP_URL=https://api.fixmymind.org
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=error

DB_CONNECTION=mysql
DB_HOST=production-db-host
DB_PORT=3306
DB_DATABASE=fixmymind_prod
DB_USERNAME=fixmymind_user
DB_PASSWORD=PRODUCTION_DB_PASSWORD

SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=true
SESSION_PATH=/
SESSION_DOMAIN=.fixmymind.org

BROADCAST_CONNECTION=redis
FILESYSTEM_DISK=s3
QUEUE_CONNECTION=redis

CACHE_STORE=redis
CACHE_PREFIX=fixmymind_prod

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=production-redis-host
REDIS_PASSWORD=PRODUCTION_REDIS_PASSWORD
REDIS_PORT=6379
REDIS_PREFIX=laravel_database_prod
REDIS_CLUSTER=redis
REDIS_PERSISTENT=true
REDIS_DB=0
REDIS_CACHE_DB=1

MAIL_MAILER=smtp
MAIL_SCHEME=tls
MAIL_HOST=smtp.mailgun.org
MAIL_PORT=587
MAIL_USERNAME=PRODUCTION_MAIL_USERNAME
MAIL_PASSWORD=PRODUCTION_MAIL_PASSWORD
MAIL_FROM_ADDRESS="noreply@fixmymind.org"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=PRODUCTION_AWS_ACCESS_KEY
AWS_SECRET_ACCESS_KEY=PRODUCTION_AWS_SECRET_KEY
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=fixmymind-prod-bucket
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
API_DOC_USER=PRODUCTION_API_DOC_USER
API_DOC_PASSWORD=PRODUCTION_API_DOC_PASSWORD

OPENAI_API_KEY=PRODUCTION_OPENAI_API_KEY
ELEVENLABS_API_KEY=PRODUCTION_ELEVENLABS_API_KEY
ELEVENLABS_VOICE_ID=PRODUCTION_VOICE_ID

STRIPE_KEY=pk_live_PRODUCTION_STRIPE_PUBLISHABLE_KEY
STRIPE_SECRET=sk_live_PRODUCTION_STRIPE_SECRET_KEY
STRIPE_WEBHOOK_SECRET=whsec_PRODUCTION_WEBHOOK_SECRET

SUPABASE_URL=https://PRODUCTION_PROJECT_ID.supabase.co
SUPABASE_KEY=PRODUCTION_SUPABASE_ANON_KEY
SUPABASE_JWK_URL=https://PRODUCTION_PROJECT_ID.supabase.co/auth/v1/keys

TELESCOPE_ENABLED=false
PULSE_ENABLED=true
